price * item.quantity,\r\n    0\r\n  );\r\n\r\n  const [checkout, setCheckOut] = useState(false);\r\n\r\n  return (\r\n    <Table responsive>\r\n      <thead>\r\n        <tr>\r\n          <th className=\"text-left\">Product</th>\r\n          <th>Price</th>\r\n          <th>Quantity</th>\r\n          <th>Total</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        {cartData.map((cartItem) => {\r\n          return <CartItem item={cartItem} key={cartItem.product_id} />;\r\n        })}\r\n      </tbody>\r\n      <tfoot>\r\n        <tr>\r\n          <td className=\"text-left\" colSpan=\"3\">\r\n            <b>Total</b>\r\n          </td>\r\n          <td>{cartTotal}</td>\r\n        </tr>\r\n        <tr>\r\n          <td colSpan=\"4\" className=\"text-right\">\r\n            <DevBtn styleClass=\"btn-dark\" type=\"button\" value=\"Checkout\">\r\n              Checkout\r\n            </DevBtn>\r\n\r\n            {checkout ? (\r\n              <PayPal />\r\n            ) : (\r\n              <DevBtn\r\n                styleClass=\"btn-dark\"\r\n                onClick={() => {\r\n                  setCheckOut(true);\r\n                }}\r\n              >\r\n                Paypal Checkout\r\n              </DevBtn>\r\n            )}\r\n          </td>\r\n        </tr>\r\n      </tfoot>\r\n    </Table>\r\n  );\r\n}\r\n\r\nexport default Cart;\r\n","C:\\Users\\sarah\\Documents\\Bootcamp\\bootcamp_homework\\Devfit\\client\\src\\components\\storePageComponents\\StoreComponent.js",[],"C:\\Users\\sarah\\Documents\\Bootcamp\\bootcamp_homework\\Devfit\\client\\src\\components\\memberPageComponents\\meetYourTrainerBox\\TrainerCarousel.js",[],"C:\\Users\\sarah\\Documents\\Bootcamp\\bootcamp_homework\\Devfit\\client\\src\\components\\memberPageComponents\\memberSchedule\\memberSchedule.js",[],"C:\\Users\\sarah\\Documents\\Bootcamp\\bootcamp_homework\\Devfit\\client\\src\\components\\managerComponents\\managerInfoBoxColumns\\mInfoBoxLeftCol.js",[],"C:\\Users\\sarah\\Docum