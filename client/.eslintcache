s-> \", actions, \" err-> \", err);\n          return actions.order.create({\n            intent: \"CAPTURE\",\n            purchase_units: [\n              {\n                description: \"Item being purchased\",\n                amount: {\n                  currency_code: \"USD\",\n                  value: total,\n                  breakdown: {\n                    item_total: {\n                      currency_code: \"USD\",\n                      value: total,\n                    },\n                  },\n                },\n                items: itemArray,\n              },\n            ],\n          });\n        },\n        //On order approval, return to store page, and show modal\n        onApprove: async (data, actions) => {\n          const order = await actions.order.capture().then(function (details) {\n            setShow(true);\n          });\n          setOrderId(order.id);\n\n          console.log(order);\n\n          let orderdata = {\n            member_id: userInfo._id,\n            order_details: [...items],\n            purchase_method: \"Paypal\",\n          };\n          console.log(\"orderdata\", orderdata);\n          fetch(\"/api/store/order\", {\n            method: \"POST\",\n            headers: {\n              \"Content-Type\": \"application/json\",\n              Accept: \"application/json\",\n            },\n            body: JSON.stringify(orderdata),\n          })\n            .then((resp) => {\n              console.log(\"Saved the Order to the DB\", resp);\n            })\n            .catch((err) => console.log(\"Error saving the order \", err));\n        },\n        onError: (err) => {\n          console.log(\"onError err-> \", err);\n        },\n      })\n      .render(paypal.current);\n  }, []);\n\n  return (\n    <div>\n      <div ref={paypal}></div>\n      <Modal show={show} onHide={handleClose}>\n        <Modal.Header closeButton>\n          <Modal.Title>Order Processed!</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          Thank you for your order! Your order id is {orderId}\n        </Modal.Body>\n        <Modal.Footer>\n          <DevBtn\n            styleClass=\"btn-red\"\n            onClick={() => {\n              handleClose();\n              setSendClasses(<Redirect to={`/member`} />);\n            }}\n          >\n            Close\n          </DevBtn>\n          {sendClasses ? sendClasses : null}\n        </Modal.Footer>\n      </Modal>\n    </div>\n  );\n}\n","C:\\Users\\Ethan's PC\\Desktop\\UNH\\My Work\\Devfit\\client\\src\\utilities\\convertTime.js",[],"C:\\Users\\Ethan's PC\\Desktop\\UNH\\My Work\\Devfit\\client\\src\\components\\commonComponents\\scheduleColumn\\scheduleColumn.js",[],"C:\\Users\\Ethan's PC\\Desktop\\UNH\\My Work\\Devfit\\client\\src\\components\\authenticationButton\\logoutButton\\logoutButton.js",[],"C:\\Users\\Ethan's PC\\Desktop\\UNH\\My Work\\Devfit\\client\\src\\components\\commonComponents\\scheduleClass\\scheduleClass.js",[],"C:\\Users\\Ethan's PC\\Desktop\\UNH\\My Work\\Devfit\\client\\src\\components\\authenticationButton\\loginButton\\loginButton.js",[],{"ruleId":"180","replacedBy":"181"},{"ruleId":"182","replacedBy":"183"},{"ruleId":"184","severity":1,"message":"185","line":1,"column":16,"nodeType":"186","messageId":"187","endLine":1,"endColumn":24},{"ruleId":"188","severity":1,"message":"189","line":86,"column":6,"nodeType":"190","endLine":86,"endColumn":8,"suggestions":"191"},"no-native-reassign",["192"],"no-negated-in-lhs",["193"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'props' and 'userInfo._id'. Either include them or remove the dependency array.","ArrayExpression",["194"],"no-global-assign","no-unsafe-negation",{"desc":"195","fix":"196"},"Update the dependencies array to be: [props, userInfo._id]",{"range":"197","text":"198"},[2876,2878],"[props, userInfo._id]"]